<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.prathamesh.hibernate.hibernatedemo.Student" table="STUD_INFORMATION">
        <id name="studRollNo" type="int">
            <column name="STUD_ROLLNO"/>
        </id>
        <property name="studName" type="string">
            <column name="STUD_NAME" length="20" not-null="true" />
        </property>
    </class>
</hibernate-mapping>